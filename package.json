{
  "name": "browseclipboard",
  "displayName": "BrowseClipboard",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "browseclipboard.copy",
        "title": "Copy"
      },
      {
        "command": "browseclipboard.cut",
        "title": "Cut"
      },
      {
        "command": "browseclipboard.quickpick.open",
        "title": "Open QuickPick"
      },
      {
        "command": "browseclipboard.quickpick.pasteonly",
        "title": "Paste from QuickPick"
      },
      {
        "command": "browseclipboard.quickpick.copy",
        "title": "Copy from QuickPick"
      },
      {
        "command": "browseclipboard.quickpick.cut",
        "title": "Cut from QuickPick"
      },
      {
        "command": "browseclipboard.quickpick.delete",
        "title": "Delete from QuickPick"
      }
    ],
    "keybindings": [
			{
				"command": "browseclipboard.copy",
				"key": "ctrl+c",
				"mac": "cmd+c",
				"when": "editorTextFocus"
			},
      {
				"command": "browseclipboard.cut",
				"key": "ctrl+x",
				"mac": "cmd+x",
				"when": "editorTextFocus"
			},
      {
				"command": "browseclipboard.openQuickPick",
				"key": "ctrl+y",
				"mac": "ctrl+y",
				"when": "editorTextFocus"
			},
      {
				"command": "browseclipboard.quickpick.pasteonly",
				"key": "shift+insert",
				"mac": "shift+insert",
				"when": "inQuickOpen"
			},
      {
				"command": "browseclipboard.quickpick.copy",
				"key": "ctrl+insert",
				"mac": "ctrl+insert",
				"when": "inQuickOpen"
			},
      {
				"command": "browseclipboard.quickpick.copy",
				"key": "ctrl+c",
				"mac": "ctrl+c",
				"when": "inQuickOpen"
			},
      {
				"command": "browseclipboard.quickpick.cut",
				"key": "ctrl+x",
				"mac": "ctrl+x",
				"when": "inQuickOpen"
			},
      {
				"command": "browseclipboard.quickpick.delete",
				"key": "ctrl+delete",
				"mac": "ctrl+delete",
				"when": "inQuickOpen"
			},
      {
				"command": "browseclipboard.quickpick.delete",
				"key": "ctrl+d",
				"mac": "ctrl+d",
				"when": "inQuickOpen"
			}
		]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.85.0",
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "typescript": "^5.3.3",
    "@vscode/test-cli": "^0.0.4",
    "@vscode/test-electron": "^2.3.8"
  }
}
